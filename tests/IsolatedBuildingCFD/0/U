FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (12.3500 0 0);

boundaryField
{
    inlet
    {
	 type 	 fixedValue;
	 value 	 uniform (12.3500 0 0);
    }

    outlet
    {
	 type 	 pressureInletOutletVelocity;
	 value 	 uniform (12.3500 0 0);
    }

    ground 
    {
	 type 	 uniformFixedValue;
	 value 	 uniform (0 0 0);
	 uniformValue 	 constant (0 0 0);
    }
    
    top    
    {
	 type    symmetry;
    }

    front    
    {
	 type 	 symmetry;
    }
    
    back    
    {
	 type    symmetry;
    }
    
    building 
    {
	 type 	 noSlip;
    }
}

// ************************************************************************* //
