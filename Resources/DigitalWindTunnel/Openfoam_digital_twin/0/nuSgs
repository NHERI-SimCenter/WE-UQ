FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuSgs;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.05;
    }

    outlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      uniform 0;
    }

    bottom
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0.05;
    }

    top
    {
        type    symmetryPlane;
    }
    
    leftWall
    {
        type    symmetryPlane;
    }

    rightWall
    {
        type    symmetryPlane;
    }

    building
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0.05;
    }
}

// ************************************************************************* //
